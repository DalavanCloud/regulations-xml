  <changeset leftDocumentNumber="2011-31714" rightDocumentNumber="2013-22752">

    <!-- Supplement I to Part 1002â€”Official Interpretations 14(b)(3) Valuation. -->
    <change operation="added" label="1002-14-b-3-Interp" parent="1002-14-b">
      <paragraph label="1002-14-b-3-Interp" marker="3.">
        <content>Valuation.</content>
      </paragraph>
    </change>

    <change operation="added" label="1002-14-b-3-Interp-1">
      <paragraph label="1002-14-b-3-Interp-1" marker="1.">
        <content>VALUATIONS-EXAMPLES.</content>
      </paragraph>
    </change>

    <change operation="added" label="1002-14-b-3-Interp-1-i" parent="1002-14-b-3-Interp-1">
      <paragraph label="1002-14-b-3-Interp-1-i" marker="i.">
        <content>A report prepared by an appraiser (whether or not licensed or certified) including the appraiser's estimate of the property's value or opinion of value.</content>
      </paragraph>
    </change>

    <change operation="added" label="1002-14-b-3-Interp-3">
      <paragraph label="1002-14-b-3-Interp-3" marker="3.">
        <content>OTHER DOCUMENTATION.</content>
      </paragraph>
    </change>

    <change operation="added" label="1002-14-b-3-Interp-3-v" parent="1002-14-b-3-Interp-3">
      <paragraph label="1002-14-b-3-Interp-3-v" marker="v.">
        <content>Reports reflecting property inspections that do not provide an estimate of the value of the property and are not used to develop an estimate of the value of the property.</content>
      </paragraph>
    </change>

    <change operation="added" label="1002-14-b-3-Interp-3-vi" parent="1002-14-b-3-Interp-3">
      <paragraph label="1002-14-b-3-Interp-3-vi" marker="vi.">
        <content>Appraisal reviews that do not include the appraiser's estimate of the property's value or opinion of value.</content>
      </paragraph>
    </change>

    <!-- Appendix A to part 1002 is amended by revising paragraph 2.d to read as follows: -->
    <change operation="modified" label="1002-A-1">
      <appendixSection label="1002-A-1" appendixSecNum="1">
        <subject></subject>
        <paragraph marker="d." label="1002-A-1-p2">
            <content>Federal Credit Unions: National Credit Union Administration, Office of Consumer Protection, 1775 Duke Street, Alexandria, VA 22314.</content>
        </paragraph>
      </appendixSection>
    </change>
